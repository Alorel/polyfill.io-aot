{
  "name": "polyfill-io-aot",
  "version": "4.0.2",
  "description": "Combines sources from the polyfill-library package",
  "main": "index.js",
  "engines": {
    "node": ">=11.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "polyfill",
    "bundler",
    "polyfill.io",
    "polyfill-library",
    "polyfill library",
    "aot",
    "polyfill bundler"
  ],
  "scripts": {
    "sync-dep-versions": "node build/sync-dependency-versions.js",
    "sync-pkg-fields": "node build/sync-package-fields.js",
    "sync-npmignores": "node build/sync-npmignores.js",
    "sync": "npm --scripts-prepend-node-path=auto run sync-pkg-fields && npm --scripts-prepend-node-path=auto run sync-dep-versions && npm --scripts-prepend-node-path=auto run sync-npmignores",
    "clean": "node build/clean.js",
    "pretsc": "npm --scripts-prepend-node-path=auto run clean",
    "doctoc": "doctoc README.md --github",
    "tsc": "tsc",
    "typecheck": "tsc --noEmit",
    "tsc:watch": "npm --scripts-prepend-node-path=auto run tsc -- --watch",
    "pretest": "npm --scripts-prepend-node-path=auto run clean",
    "clean:dts": "node build/clean-extraneous-declarations.js",
    "test": "nyc mocha --opts ./mocha.opts test",
    "tslint": "alo tslint -p ./tsconfig.json ",
    "tslint:fix": "npm --scripts-prepend-node-path=auto run tslint -- --fix"
  },
  "author": "Alorel <a.molcanovas@gmail.com> (https://github.com/Alorel)",
  "license": "MIT",
  "devDependencies": {
    "@alorel-personal/build-tools": "4.5.6",
    "@alorel-personal/conventional-changelog-alorel": "2.1.0",
    "@alorel-personal/semantic-release": "1.4.0",
    "@alorel-personal/tslint-rules": "4.1.1",
    "@semantic-release/changelog": "~3.0.0",
    "@semantic-release/exec": "~3.3.2",
    "@semantic-release/git": "~7.0.1",
    "@semantic-release/github": "~5.2.10",
    "@semantic-release/npm": "~5.1.4",
    "@types/bluebird": "~3.5.20",
    "@types/chai": "~4.1.4",
    "@types/cross-spawn": "~6.0.0",
    "@types/etag": "~1.8.0",
    "@types/express": "~4.16.0",
    "@types/fs-extra": "~7.0.0",
    "@types/glob": "~7.1.0",
    "@types/json5": "~0.0.30",
    "@types/lodash": "~4.14.123",
    "@types/mocha": "~5.2.3",
    "@types/moment-timezone": "~0.5.6",
    "@types/node": "~12.0.0",
    "@types/node-zopfli-es": "~0.0.0",
    "@types/supertest": "~2.0.5",
    "@types/tmp": "~0.1.0",
    "@types/workerpool": "~2.3.0",
    "@types/yargs": "~13.0.0",
    "bluebird": "~3.5.1",
    "chai": "~4.2.0",
    "coveralls": "~3.0.1",
    "cross-spawn": "~6.0.5",
    "doctoc": "~1.4.0",
    "etag": "~1.8.1",
    "express": "~4.16.3",
    "fs-extra": "~8.0.0",
    "glob": "~7.1.2",
    "json5": "~2.1.0",
    "lazy-get-decorator": "~2.2.0",
    "lodash": "~4.17.10",
    "mocha": "~6.1.1",
    "moment-timezone": "~0.5.23",
    "node-zopfli-es": "~1.0.0",
    "nyc": "~14.1.0",
    "ora": "~3.4.0",
    "polyfill-library": "~3.34.0",
    "semantic-release": "~15.13.3",
    "source-map-support": "~0.5.10",
    "supertest": "~4.0.0",
    "terser": "~3.17.0",
    "tmp": "~0.1.0",
    "ts-node": "~7.0.1",
    "tslib": "~1.9.1",
    "typescript": "~3.4.1",
    "workerpool": "~3.2.0 || 3.1.1",
    "yargs": "~13.2.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": "git@github.com:Alorel/polyfill.io-aot.git",
  "private": true,
  "peerDependencies": {
    "@types/express": "^4.0.0",
    "@types/moment-timezone": ">=0.5.0 <1.0.0",
    "@types/node": "~12.0.0",
    "@types/node-zopfli-es": ">=0.0.0 <1.0.0",
    "bluebird": "^3.0.0",
    "cross-spawn": "^6.0.0",
    "fs-extra": "^8.0.0",
    "json5": "^2.1.0",
    "lazy-get-decorator": "^2.2.0",
    "lodash": "^4.0.0",
    "node-zopfli-es": "^1.0.0",
    "polyfill-library": "^3.34.0",
    "tslib": "^1.6.0",
    "workerpool": "~3.2.0 || 3.1.1",
    "yargs": "^13.1.0"
  }
}
